@{
}
<h2>List of Article</h2>

<a href="/Editor/Create"><button class="btn-outline-primary float-right">+ NEW Schedule</button></a>

<table class="table">
    <thead>
        <tr>
            <th>Doc ID</th>
            <th>Content </th>
            <th> Edit / View / Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (EditorModel editor in @ViewBag.EditorList)
        {
            <tr>
                <td>@editor.DocID</td>
                <td>@Html.Raw(editor.ContentDetails)</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@editor.DocID"><button class="btn btn-outline-secondary" type="submit">Edit</button></a>
                    <a asp-action="View" asp-route-id="@editor.DocID"><button class="btn btn-outline-primary" type="submit">View</button></a>
                    <a asp-action="Delete" asp-route-id="@editor.DocID"><button class="btn btn-outline-danger" type="submit">Delete</button></a>


                </td>
            </tr>
        }
    </tbody>
</table>


